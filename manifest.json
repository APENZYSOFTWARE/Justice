{
  "manifest_version": 3,
  "name": "Justice Fashion Quick Access",
  "version": "1.0",
  "description": "Quick access to Justice Fashion website",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Justice Fashion",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
